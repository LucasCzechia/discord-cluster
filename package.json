{
	"name": "discord-cluster",
	"version": "1.0.0",
	"description": "Transparent cross-cluster operations for discord.js bots. Cache first, REST fallback, type-safe IPC, shared state, and more.",
	"author": "LucasCzechia",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsc --build",
		"watch": "tsc --watch",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LucasCzechia/discord-cluster"
	},
	"bugs": {
		"url": "https://github.com/LucasCzechia/discord-cluster/issues"
	},
	"homepage": "https://discord-cluster.vercel.app",
	"keywords": [
		"discord",
		"discord.js",
		"sharding",
		"clustering",
		"discord-sharding",
		"discord-bot",
		"discord-cluster",
		"discord-sharding-bot",
		"discord-bot-sharding",
		"discord-bot-sharding-example",
		"discord-sharding-example",
		"discord.js-sharding-example",
		"discord.js-bot-sharding-example",
		"discord.js-sharding",
		"discord.js-bot-sharding",
		"discord.js-cluster",
		"discord.js-clustering"
	],
	"devDependencies": {
		"@types/colors": "^1.1.3",
		"@types/node": "22.13.1",
		"discord.js": "^14.22.1",
		"typescript": "5.7.3"
	},
	"peerDependencies": {
		"discord.js": ">=14.14.1"
	},
	"dependencies": {
		"colors": "^1.4.0"
	},
	"engines": {
		"node": ">=18.4.0"
	},
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	]
}
